<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:textColorPrimary">@color/textColorPrimary</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <!--Normal-->
    <style name="layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="ygsClasses">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/blueMidnight</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:typeface">serif</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="edittextLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">16sp</item>
    </style>

    <style name="edittext">
        <item name="android:layout_width">50sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLength">5</item>
        <item name="android:inputType">numberDecimal</item>
        <item name="android:textColor">@color/blueMidnight</item>
        <item name="android:typeface">serif</item>
        <item name="android:textColorHint">@color/blueMidnight</item>
        <item name="android:textColorHighlight">@color/background</item>
        <item name="android:backgroundTint">@color/blueMidnight</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
    </style>

